                                <script>

                                    "use strict";

                                    var ACCORDION_NAMES_TO_OFFER_SPECIFIC_ELEMENTS = {
                                        "renewedTier1AccordionRow": "RENEWED_TIER_1_offerConsistency",
                                        "renewedTier2AccordionRow": "RENEWED_TIER_2_offerConsistency",
                                        "renewedTier3AccordionRow": "RENEWED_TIER_3_offerConsistency",
                                        "renewedTier4AccordionRow": "RENEWED_TIER_4_offerConsistency",

                                        "mobileApp_renewedTier1AccordionRow": "RENEWED_TIER_1_offerConsistency",
                                        "mobileApp_renewedTier2AccordionRow": "RENEWED_TIER_2_offerConsistency",
                                        "mobileApp_renewedTier3AccordionRow": "RENEWED_TIER_3_offerConsistency",
                                        "mobileApp_renewedTier4AccordionRow": "RENEWED_TIER_4_offerConsistency",

                                        "mobile_renewedTier1AccordionRow": "RENEWED_TIER_1_offerConsistency",
                                        "mobile_renewedTier2AccordionRow": "RENEWED_TIER_2_offerConsistency",
                                        "mobile_renewedTier3AccordionRow": "RENEWED_TIER_3_offerConsistency",
                                        "mobile_renewedTier4AccordionRow": "RENEWED_TIER_4_offerConsistency"
                                    };

                                    P.when('A', 'ready').execute(function(A) {

                                        function updateRenewedOfferConsistency(selectedAccordionRowName) {

                                            for (var [key,elementName] of Object.entries(ACCORDION_NAMES_TO_OFFER_SPECIFIC_ELEMENTS)) {
                                                var offerSpecificElements = A.$('.' + elementName);
                                                offerSpecificElements.css('display', 'none');
                                            }

                                            var elementToShow = ACCORDION_NAMES_TO_OFFER_SPECIFIC_ELEMENTS[selectedAccordionRowName];
                                            if (typeof elementToShow === 'undefined') {
                                                return;
                                            }

                                            A.$('.' + elementToShow).css('display', 'block')
                                        }

                                        var initialSelectedAccordionRowElement = A.$('#buybox .a-accordion-active');

                                        if (typeof initialSelectedAccordionRowElement === 'undefined' || !initialSelectedAccordionRowElement || typeof initialSelectedAccordionRowElement[0] === 'undefined' || !initialSelectedAccordionRowElement[0] || typeof initialSelectedAccordionRowElement[0].id === 'undefined' || !initialSelectedAccordionRowElement[0].id) {
                                            return;
                                        }

                                        var initialSelectedAccordionRowName = initialSelectedAccordionRowElement[0].id;
                                        updateRenewedOfferConsistency(initialSelectedAccordionRowName);

                                        A.on('a:accordion:buybox-accordion:select', function(data) {
                                            var selectedAccordionRowName = data.selectedRow.rowName;
                                            updateRenewedOfferConsistency(selectedAccordionRowName);
                                        });

                                    });
                                </script>
