                    }
                });

                var CSMReqs = {
                    af: {
                        c: 2,
                        p: 'atf'
                    },
                    cf: {
                        c: 2,
                        p: 'cf'
                    },
                    x1: {
                        c: 1,
                        p: 'x1'
                    },
