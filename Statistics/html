                                                                    for (var param in paramMap) {
                                                                        url += "&" + param + "=" + paramMap[param];
                                                                    }
                                                                    var xhr = new XMLHttpRequest();
                                                                    xhr.open("POST", url, false);
                                                                    xhr.setRequestHeader("anti-csrftoken-a2z", "g6ZcJdFlfLFdIe31NWJud8OsrZepzo9j82fBjodPA9+SAAAAAQAAAABkkrbTcmF3AAAAAHuL9oHQYR32uqP6iUf8kQ==");
